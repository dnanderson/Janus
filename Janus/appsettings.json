{
  "Database": {
    "Url": "your_production_database_url"
  },
  "SerialService": {
    "Type": "Ni845x"
  },
  "Drawers": [
    {
      "DrawerId": 1,
      "SmuResourceName": "SMU1",
      "DaqResourceName": "DAQ1",
      "SerialDevice": {
        "Port": "COM1",
        "BaudRate": 9600
      }
    },
    {
      "DrawerId": 2,
      "SmuResourceName": "SMU2",
      "DaqResourceName": "DAQ2",
      "SerialDevice": {
        "Port": "COM2",
        "BaudRate": 19200
      }
    }
  ],
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [ "FromLogContext" ],
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "logs/log.txt",
          "rollingInterval": "Day",
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] {Message:lj}{NewLine}{Exception}"
        }
      },
      {
        "Name": "Logger",
        "Args": {
          "configureLogger": {
            "Filter": [
              {
                "Name": "ByIncludingOnly",
                "Args": {
                  "expression": "Properties.CorrelationId is not null"
                }
              }
            ],
            "WriteTo": [
              {
                "Name": "File",
                "Args": {
                  "path": "logs/tests/log-.txt",
                  "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] {Message:lj}{NewLine}{Exception}",
                  "pathFormat": "logs/tests/{CorrelationId}.log"
                }
              }
            ]
          }
        }
      }
    ]
  }
}
